{
    "schemaVersion": 1,
    "id": "blockui",
    "version": "${version}",
    "name": "BlockUI",
    "description": "UI Library mod, use ctrl+alt+shift+x to open a test-ui",
    "authors": [
        "LDT Team",
        "BluSpring"
    ],
    "contact": {
        "repo": "https://github.com/BluSpring/BlockUI"
    },
    "license": "GPL-3.0-only",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "com.ldtteam.blockui.mod.BlockUI"
        ]
    },
    "mixins": [
        "blockui.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric-api": ">=${fabric_version}",
        "minecraft": "${minecraft_version}",
        "architectury": ">=${architectury_version}"
    },
    "accessWidener": "blockui.accesswidener"
}